<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <PackageId>ServiceStack.Logging.EntLib5</PackageId>
    <AssemblyName>ServiceStack.Logging.EntLib5</AssemblyName>
    <TargetFramework>net45</TargetFramework>
    <Title>ServiceStack.Logging.EntLib5</Title>
    <PackageDescription>
      Provides Enterprise Library 5.0 Logging Application Block logging integration for other ServiceStack projects
    </PackageDescription>
    <PackageTags>servicestack;log;logging;EntLib5</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\ServiceStack.Interfaces\ServiceStack.Interfaces.csproj" />
    <PackageReference Include="EnterpriseLibrary.Logging" Version="5.0.505.1" />
    <PackageReference Include="ServiceStack.Text" Version="$(Version)" />
    <Reference Include="System" />
    <Reference Include="System.Configuration" />
    <Reference Include="Microsoft.CSharp" />
  </ItemGroup>

</Project>
