<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <PackageId>ServiceStack.Logging.EventLog</PackageId>
    <AssemblyName>ServiceStack.Logging.EventLog</AssemblyName>
    <TargetFramework>net45</TargetFramework>
    <Title>ServiceStack.Logging.EventLog</Title>
    <PackageDescription>
      Provides EventLog logging integration for other ServiceStack projects
    </PackageDescription>
    <PackageTags>servicestack;log;logging;EventLog</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\ServiceStack.Interfaces\ServiceStack.Interfaces.csproj" />
    <PackageReference Include="ServiceStack.Text" Version="$(Version)" />
    <Reference Include="System" />
    <Reference Include="System.Configuration" />
    <Reference Include="Microsoft.CSharp" />
  </ItemGroup>

</Project>
