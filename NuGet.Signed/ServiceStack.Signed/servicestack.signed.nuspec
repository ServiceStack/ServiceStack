    <copyright>Copyright 2016 Service Stack</copyright>
    <dependencies>
      <dependency id="ServiceStack.Common.Signed" version="4.5" />
      <dependency id="ServiceStack.Client.Signed" version="4.5" />
    </dependencies>
  </metadata>
  <files>
    <file src="lib\**\*.*" target="lib" />
    <file src="..\..\src\ServiceStack\**\*.cs" target="src\ServiceStack" />
    <file src="..\..\src\ServiceStack\**\*.js" target="src" />
    <file src="..\..\src\ServiceStack\**\*.css" target="src" />
    <file src="..\..\src\ServiceStack\**\*.html" target="src" />
    <file src="..\..\src\ServiceStack\**\*.tmpl" target="src" />
