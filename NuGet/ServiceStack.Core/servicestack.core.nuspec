<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>ServiceStack.Core</id>
    <title>ServiceStack .NET Standard 2.0</title>
    <version>5.0.0</version>
    <authors>ServiceStack</authors>
    <owners>ServiceStack</owners>
    <description>
      .NET Standard 2.0 version of ServiceStack
    </description>
    <projectUrl>https://github.com/ServiceStack/ServiceStack</projectUrl>
    <licenseUrl>https://servicestack.net/terms</licenseUrl>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <iconUrl>https://servicestack.net/img/logo-32.png</iconUrl>
    <tags>Fast JSON XML CSV HTML SOAP JSV REST Web Service Framework MONO ServiceStack</tags>
    <language>en-US</language>
    <copyright>ServiceStack and contributors</copyright>
    <dependencies>
      <group targetFramework=".netstandard2.0">
        <dependency id="Microsoft.AspNetCore.Http" version="[2.0.0, )" />
        <dependency id="Microsoft.AspNetCore.Http.Abstractions" version="[2.0.0, )" />
        <dependency id="Microsoft.AspNetCore.Http.Extensions" version="[2.0.0, )" />
        <dependency id="Microsoft.AspNetCore.Hosting.Abstractions" version="[2.0.0, )" />
        <dependency id="Microsoft.Extensions.Logging.Abstractions" version="[2.0.0, )" />
        <dependency id="Microsoft.Extensions.DependencyInjection.Abstractions" version="[2.0.0, )" />
        <dependency id="Microsoft.Extensions.Primitives" version="[2.0.0, )" />
        <dependency id="System.Threading.Thread" version="[4.3.0, )" />
        <dependency id="System.Reflection.Emit" version="[4.3.0, )" />
        <dependency id="System.Linq.Queryable" version="[4.3.0, )" />
        <dependency id="ServiceStack.Client.Core" version="5.0.0" />
        <dependency id="ServiceStack.Common.Core" version="5.0.0" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="lib\**\*.*" target="lib" />
    <file src="..\..\src\ServiceStack\**\*.cs" target="src" />
    <file src="..\..\src\ServiceStack\**\*.js" target="src" />
    <file src="..\..\src\ServiceStack\**\*.ts" target="src" />
    <file src="..\..\src\ServiceStack\**\*.css" target="src" />
    <file src="..\..\src\ServiceStack\**\*.html" target="src" />
  </files>
</package>