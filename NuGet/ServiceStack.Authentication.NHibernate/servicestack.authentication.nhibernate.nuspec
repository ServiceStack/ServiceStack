<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>ServiceStack.Authentication.NHibernate</id> 
    <version>5.0.0</version>
    <authors>ServiceStack</authors> 
    <owners>ServiceStack</owners>
    <summary>An NHibernate IUserAuthRepository to persist User registration and auth info in any RDBMS NHibernate supports</summary>
    <description>
        An NHibernate IUserAuthRepository to persist User registration and authentication info in any RDBMS NHibernate supports.
        Implements ServiceStack's built-in AuthProvider Model:
        http://docs.servicestack.net/authentication-and-authorization
        inc. Twitter, Facebook, Basic, Digest, Credentials Auth modes out-of-the-box.
    </description> 
    <projectUrl>https://github.com/ServiceStack/ServiceStack</projectUrl>
    <licenseUrl>https://servicestack.net/terms</licenseUrl>
    <iconUrl>https://servicestack.net/img/logo-32.png</iconUrl>
    <tags>ServiceStack Authentication Authorization NHibernate SQLServer Sqlite Oracle MySql PostgreSQL POCO Code-First</tags>
    <language>en-US</language> 
    <copyright>ServiceStack and contributors</copyright>
    <dependencies>
      <group targetFramework="net45">
        <dependency id="NHibernate" version="4.1.1.4000" />
        <dependency id="Iesi.Collections" version="4.0.2" />
        <dependency id="FluentNHibernate" version="2.0.3.0" />
        <dependency id="ServiceStack" version="5.0.0" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="lib\**\*.*" target="lib" />
    <file src="..\..\src\ServiceStack.Authentication.NHibernate\**\*.cs" target="src" />
  </files>
</package>
