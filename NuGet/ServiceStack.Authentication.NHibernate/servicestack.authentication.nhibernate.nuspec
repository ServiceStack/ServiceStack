    <tags>ServiceStack Authentication Authorization NHibernate SQLServer Sqlite Oracle MySql PostgreSQL POCO Code-First</tags>
    <language>en-US</language> 
    <copyright>ServiceStack 2014 and contributors</copyright>
    <dependencies>
      <dependency id="ServiceStack" version="4.0" />
      <dependency id="NHibernate" version="4.0.1.4000" />
      <dependency id="Iesi.Collections" version="4.0.1.4000" />
      <dependency id="FluentNHibernate" version="1.4.0.0" />
    </dependencies>
  </metadata>
