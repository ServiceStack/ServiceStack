        <dependency id="ServiceStack.Interfaces" version="5.0.0" />
        <dependency id="ServiceStack.Text" version="5.0.0" />
      </group>
      <group targetFramework=".netstandard2.0">
        <dependency id="Microsoft.Extensions.Primitives" version="[2.0.0, )" />
        <dependency id="System.Dynamic.Runtime" version="[4.3.0, )" />
        <dependency id="System.Data.Common" version="[4.3.0, )" />
        <dependency id="System.ComponentModel.Primitives" version="[4.3.0, )" />
        <dependency id="System.Net.Primitives" version="[4.3.0, )" />
        <dependency id="System.Net.NetworkInformation" version="[4.3.0, )" />
        <dependency id="System.Runtime.Serialization.Primitives" version="[4.3.0, )" />
        <dependency id="System.Reflection.TypeExtensions" version="[4.3.0, )" />
        <dependency id="ServiceStack.Interfaces" version="5.0.0" />
        <dependency id="ServiceStack.Text" version="5.0.0" />
