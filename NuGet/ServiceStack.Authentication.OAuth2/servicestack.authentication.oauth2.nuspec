    <licenseUrl>https://github.com/ServiceStack/ServiceStack/blob/master/LICENSE</licenseUrl>
    <iconUrl>http://www.servicestack.net/logo-100x100.png</iconUrl>
    <tags>OAuth OAuth2 Google LinkedIn Authentication Auth Provider ServiceStack Web Services</tags>
    <language>en-US</language>
    <copyright>servicestack.net 2013 and contributors</copyright>
    <dependencies>
        <dependency id="ServiceStack" version="4.0" />
        <dependency id="DotNetOpenAuth.Ultimate" />
    </dependencies> 
  </metadata>
  <files>
      <file src="lib\**\*.*" target="lib" />
      <file src="..\..\src\ServiceStack.Authentication.OAuth2\**\*.cs" target="src" />
  </files>
