<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <PackageId>ServiceStack.RabbitMq.Core</PackageId>
        <AssemblyName>ServiceStack.RabbitMq</AssemblyName>
        <RootNamespace>ServiceStack.RabbitMq</RootNamespace>
        <TargetFrameworks>netstandard2.0;net6.0;net8.0</TargetFrameworks>
        <Title>ServiceStack.RabbitMq .NET Standard 2.0</Title>
        <PackageDescription>
            Rabbit MQ client and server adapters for the ServiceStack web services framework.
            This library enables consuming and publishing to ServiceStack services when hosted within a Rabbit MQ Server Host.
        </PackageDescription>
        <PackageTags>MQ;Message;Queue;Web;Service;Framework;Fast;JSON;ServiceStack</PackageTags>
    </PropertyGroup>

    <ItemGroup>
        <ProjectReference Include="..\..\..\ServiceStack.Text\src\ServiceStack.Text\ServiceStack.Text.Core.csproj" />
        <ProjectReference Include="..\ServiceStack.Interfaces\ServiceStack.Interfaces.Core.csproj" />
        <ProjectReference Include="..\ServiceStack.Client\ServiceStack.Client.Core.csproj" />
        <ProjectReference Include="..\ServiceStack.Common\ServiceStack.Common.Core.csproj" />
        <ProjectReference Include="..\ServiceStack\ServiceStack.Core.csproj" />
        <PackageReference Include="RabbitMQ.Client" Version="6.8.1" />
    </ItemGroup>

</Project>
